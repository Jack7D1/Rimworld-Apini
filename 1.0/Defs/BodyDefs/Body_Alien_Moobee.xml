<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Body used by the Moobee animal. -->
  <BodyDef>
    <defName>Moobee</defName>
    <label>moobee</label>
    <corePart>
      <def>Thorax</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <groups>
        <li>Torso</li>
      </groups>
      <parts>
        <li>
          <def>InsectWing</def>
          <customLabel>left forewing</customLabel>
          <coverage>0.04</coverage>
          <depth>Outside</depth>
          <groups>
            <li>InsectWings</li>
          </groups>
        </li>
        <li>
          <def>InsectWing</def>
          <customLabel>right forewing</customLabel>
          <coverage>0.04</coverage>
          <depth>Outside</depth>
          <groups>
            <li>InsectWings</li>
          </groups>
        </li>
        <li>
          <def>InsectWing</def>
          <customLabel>left hindwing</customLabel>
          <coverage>0.04</coverage>
          <depth>Outside</depth>
          <groups>
            <li>InsectWings</li>
          </groups>
        </li>
        <li>
          <def>InsectWing</def>
          <customLabel>right hindwing</customLabel>
          <coverage>0.04</coverage>
          <depth>Outside</depth>
          <groups>
            <li>InsectWings</li>
          </groups>
        </li>
        <li>
          <def>Stomach</def>
          <coverage>0.02</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>HoneyStomach</def>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>InsectHeart</def>
          <coverage>0.04</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Pronotum</def>
          <coverage>0.07</coverage>
          <height>Top</height>
          <groups>
            <li>Neck</li>
          </groups>
          <parts>
            <li>
              <def>Head</def>
              <!--              <def>InsectHead</def>-->
              <coverage>0.75</coverage>
              <groups>
                <li>UpperHead</li>
                <li>FullHead</li>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>Brain</def>
                  <coverage>0.1</coverage>
                  <depth>Inside</depth>
                  <groups>
                    <li>UpperHead</li>
                    <li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>left eye</customLabel>
                  <coverage>0.2</coverage>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>right eye</customLabel>
                  <coverage>0.2</coverage>
                </li>
                <li>
                  <def>Antenna</def>
                  <customLabel>left antenna</customLabel>
                  <coverage>0.1</coverage>
                </li>
                <li>
                  <def>Antenna</def>
                  <customLabel>right antenna</customLabel>
                  <coverage>0.1</coverage>
                </li>
                <li>
                  <def>InsectNostril</def>
                  <coverage>0.05</coverage>
                </li>
                <li>
                  <def>InsectMouth</def>
                  <coverage>0.1</coverage>
                  <groups>
                    <li>Mouth</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Mandible</def>
                      <customLabel>left mandible</customLabel>
                      <coverage>0.08</coverage>
                    </li>
                    <li>
                      <def>Mandible</def>
                      <customLabel>right mandible</customLabel>
                      <coverage>0.08</coverage>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Abdomen</def>
          <customLabel>abdomen</customLabel>
          <coverage>0.1</coverage>
          <height>Middle</height>
          <groups>
            <li>Abdomen</li>
            <li>Waist</li>
          </groups>
          <parts>
            <li>  <!-- Sting -->
              <def>InsectSting</def>
              <coverage>0.1</coverage>
              <height>Bottom</height>
              <groups>
                <li>InsectSting</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>left front leg</customLabel>
          <coverage>0.04</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>left front tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>right front leg</customLabel>
          <coverage>0.04</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>right front tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>left middle leg</customLabel>
          <coverage>0.04</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>left middle tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>right middle leg</customLabel>
          <coverage>0.04</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>right middle tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>left rear leg</customLabel>
          <coverage>0.04</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>left rear tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>right rear leg</customLabel>
          <coverage>0.04</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>right rear tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>
  
</Defs>
