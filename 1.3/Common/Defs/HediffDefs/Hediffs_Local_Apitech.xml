		<descriptionHyperlinks>
			<ThingDef>SignalReceptor_Apini</ThingDef>
		</descriptionHyperlinks>
		<stages>
			<li>
				<partEfficiencyOffset>0.5</partEfficiencyOffset>
				<statOffsets>
					<TradePriceImprovement>0.1</TradePriceImprovement>
				</statOffsets>
				<capMods>
					<li>
						<capacity>Hearing</capacity>
						<offset>0.25</offset>
					</li>
				</capMods>
			</li>
		</stages>
	</HediffDef>
	<!-- Monarch Mandibles (Mouth) -->
	<HediffDef ParentName="ApitechMutationBase">
		<defName>EnhancedMouth_Monarch_Apini</defName>
		<label>Monarch Mandibles</label>
		<description>Installed Monarch Mandibles.</description>
		<descriptionHyperlinks>
			<ThingDef>MonarchMandibles_Apini</ThingDef>
		</descriptionHyperlinks>
		<hediffClass>Hediff_AddedPart</hediffClass>
		<stages>
			<li>
				<statOffsets>
					<SocialImpact>0.5</SocialImpact>
					<TradePriceImprovement>0.25</TradePriceImprovement>
					<TameAnimalChance>0.25</TameAnimalChance>
					<TrainAnimalChance>0.25</TrainAnimalChance>
					<MiningSpeed>0.25</MiningSpeed>
					<MiningYield>0.25</MiningYield>
				</statOffsets>
				<capMods>
					<li>
						<capacity>Eating</capacity>
						<offset>0.25</offset>
					</li>
					<li>
						<capacity>Talking</capacity>
						<offset>0.25</offset>
					</li>
				</capMods>
			</li>
		</stages>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.5</partEfficiency>
		</addedPartProps>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>monarch mandibles</label>
						<capacities>
							<li>Bite</li>
						</capacities>
						<cooldownTime>2</cooldownTime>
						<power>25</power>
					</li>
				</tools>
			</li>
		</comps>
	</HediffDef>
