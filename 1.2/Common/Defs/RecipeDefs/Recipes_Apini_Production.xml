	<RecipeDef ParentName="ApiniProstheticsTechCraft">
		<!-- Prosthetic Wing -->
		<defName>CreateProstheticWing</defName>
		<label>Craft prosthetic wing</label>
		<description>A prosthetic wing made of lightweight fabrics.</description>
		<jobString>Crafting Prosthetic wings.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Cloth</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<ProstheticWing>1</ProstheticWing>
		</products>
	</RecipeDef>
