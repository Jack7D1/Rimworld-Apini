<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Bee body used by Apini and Azuri alien races. -->
  <BodyDef>
    <defName>Apini</defName>
    <label>apini</label>
    <corePart>
      <def>Thorax</def>
      <height>Middle</height>
      <depth>Outside</depth>
      <groups>
        <li>Torso</li>
      </groups>
      <parts>
        <li>
          <def>InsectWing</def>
          <customLabel>left forewing</customLabel>
          <coverage>0.04</coverage>
          <depth>Outside</depth>
          <groups>
            <li>InsectWings</li>
          </groups>
        </li>
        <li>
          <def>InsectWing</def>
          <customLabel>right forewing</customLabel>
          <coverage>0.04</coverage>
          <depth>Outside</depth>
          <groups>
            <li>InsectWings</li>
          </groups>
        </li>
        <li>
          <def>InsectWing</def>
          <customLabel>left hindwing</customLabel>
          <coverage>0.04</coverage>
          <depth>Outside</depth>
          <groups>
            <li>InsectWings</li>
          </groups>
        </li>
        <li>
          <def>InsectWing</def>
          <customLabel>right hindwing</customLabel>
          <coverage>0.04</coverage>
          <depth>Outside</depth>
          <groups>
            <li>InsectWings</li>
          </groups>
        </li>
        <li>
          <def>Stomach</def>
          <coverage>0.02</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>HoneyStomach</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>InsectHeart</def>
          <coverage>0.03</coverage>
          <depth>Inside</depth>
          <groups>
            <li>Torso</li>
          </groups>
        </li>
        <li>
          <def>Pronotum</def>
          <coverage>0.07</coverage>
          <height>Top</height>
          <groups>
            <li>Neck</li>
          </groups>
          <parts>
            <li>
              <def>Head</def>
              <!--<def>InsectHead</def>-->
              <coverage>0.65</coverage>
              <groups>
                <li>UpperHead</li>
                <li>FullHead</li>
                <li>HeadAttackTool</li>
              </groups>
              <parts>
                <li>
                  <def>Brain</def>
                  <coverage>0.2</coverage>
                  <depth>Inside</depth>
                  <groups>
                    <li>UpperHead</li>
                    <li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>left eye</customLabel>
                  <coverage>0.15</coverage>
                  <groups>
                    <li>Eyes</li>
					<li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>Eye</def>
                  <customLabel>right eye</customLabel>
                  <coverage>0.15</coverage>
                  <groups>
                    <li>Eyes</li>
					<li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>Antenna</def>
                  <customLabel>left antenna</customLabel>
                  <coverage>0.05</coverage>
                  <groups>
                    <li>UpperHead</li>
					<li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>Antenna</def>
                  <customLabel>right antenna</customLabel>
                  <coverage>0.05</coverage>
                  <groups>
                    <li>UpperHead</li>
					<li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>InsectNostril</def>
                  <coverage>0.1</coverage>
                  <groups>
					<li>FullHead</li>
                  </groups>
                </li>
                <li>
                  <def>InsectMouth</def>
                  <coverage>0.07</coverage>
                  <groups>
                    <li>Mouth</li>
                    <li>Teeth</li> <!-- so that they don't wear 100 scarfs from Vanilla Apparel Expanded -->
                    <li>FullHead</li>
                  </groups>
                  <parts>
                    <li>
                      <def>Mandible</def>
                      <customLabel>left mandible</customLabel>
                      <coverage>0.08</coverage>
					  <groups>
						<li>Mouth</li>
						<li>Teeth</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                    <li>
                      <def>Mandible</def>
                      <customLabel>right mandible</customLabel>
                      <coverage>0.08</coverage>
					  <groups>
						<li>Mouth</li>
						<li>Teeth</li>
                        <li>FullHead</li>
                      </groups>
                    </li>
                  </parts>
                </li>
              </parts>
            </li>
          </parts>
        </li>
        <li>
          <def>Abdomen</def>
          <customLabel>abdomen</customLabel>
          <coverage>0.11</coverage>
          <height>Middle</height>
          <groups>
            <li>Abdomen</li>
			<li>Waist</li>
          </groups>
        </li>
        <li>
          <def>ApiniFrontLeg</def>
          <customLabel>left front leg</customLabel>
          <coverage>0.06</coverage>
          <height>Bottom</height>
          <groups>
            <li>ApiniFrontLeg</li>
            <li>Arms</li>
            <li>Shoulders</li> <!-- so that they don't wear 100 shoulderpads from Apparello -->
          </groups>
          <parts>
            <li>
              <def>Tarsal</def>
              <customLabel>left first tarsal claw</customLabel>
              <coverage>0.03</coverage>
              <groups>
                <li>Hands</li>
                <li>LeftHand</li>
              </groups>
            </li>
            <li>
              <def>Tarsal</def>
              <customLabel>left second tarsal claw</customLabel>
              <coverage>0.03</coverage>
              <groups>
                <li>Hands</li>
                <li>LeftHand</li>
              </groups>
            </li>
            <li>
              <def>Tarsal</def>
              <customLabel>left third tarsal claw</customLabel>
              <coverage>0.02</coverage>
              <groups>
                <li>Hands</li>
                <li>LeftHand</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>ApiniFrontLeg</def>
          <customLabel>right front leg</customLabel>
          <coverage>0.06</coverage>
          <height>Bottom</height>
          <groups>
            <li>ApiniFrontLeg</li>
            <li>Arms</li>
            <li>Shoulders</li>
          </groups>
          <parts>
            <li>
              <def>Tarsal</def>
              <customLabel>right first tarsal claw</customLabel>
              <coverage>0.03</coverage>
              <groups>
                <li>Hands</li>
                <li>RightHand</li>
              </groups>
            </li>
            <li>
              <def>Tarsal</def>
              <customLabel>right second tarsal claw</customLabel>
              <coverage>0.03</coverage>
              <groups>
                <li>Hands</li>
                <li>RightHand</li>
              </groups>
            </li>
            <li>
              <def>Tarsal</def>
              <customLabel>right third tarsal claw</customLabel>
              <coverage>0.02</coverage>
              <groups>
                <li>Hands</li>
                <li>RightHand</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>left middle leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>left middle tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>right middle leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>right middle tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>left rear leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>left rear tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
        <li>
          <def>InsectLeg</def>
          <customLabel>right rear leg</customLabel>
          <coverage>0.05</coverage>
          <height>Bottom</height>
          <groups>
            <li>Legs</li>
            <li>HindLegs</li>
          </groups>
          <parts>
            <li>
              <def>Tarsus</def>
              <customLabel>right rear tarsus</customLabel>
              <coverage>0.04</coverage>
              <height>Bottom</height>
              <groups>
                <li>Feet</li>
              </groups>
            </li>
          </parts>
        </li>
      </parts>
    </corePart>
  </BodyDef>
  
</Defs>
