	<RecipeDef Name="NectarCraftBase" Abstract="True">
		<jobString>distilling Nectar.</jobString>
		<workSpeedStat>DrugCookingSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<workAmount>400</workAmount>
		<workSkillLearnFactor>5</workSkillLearnFactor>
		<requiredGiverWorkType>HoneyMaker</requiredGiverWorkType>
		<workSkill>Cooking</workSkill>
		<recipeUsers>
			<li>ApiniNectarStill</li>
		</recipeUsers>
		<fixedIngredientFilter>
			<categories>
				<li>Pollen</li>
			</categories>
			<!--			<exceptedThingDefs>
				<li>Pollen</li>
				<!{1}**Bland pollen containing no nectar, which are in the same thingcategory as pollenballs**{1}>
			</exceptedThingDefs>-->
		</fixedIngredientFilter>
		<specialProducts>
			<li>Smelted</li>
		</specialProducts>
	</RecipeDef>
