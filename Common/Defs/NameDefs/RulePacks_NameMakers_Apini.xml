<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonApini</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[apini] [flower]</li>
        <li>name->[apini] [flower]</li>
        <li>name->[apini] '[positive]' [flower]</li>
        <li>name->[color] '[vegetable]' [flower]</li>
        <li>name->[apini] '[vegetable]' [roman]</li>
        <li>name->[apini] '[positive]' [roman]</li>

      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>apini</keyword>
          <path>ApiniNames/Apini</path>
        </li>
        <li Class="Rule_File">
          <keyword>flower</keyword>
          <path>ApiniNames/Flower</path>
        </li>
        <li Class="Rule_File">
          <keyword>vegetable</keyword>
          <path>ApiniNames/Vegetables</path>
        </li>
        <li Class="Rule_File">
          <keyword>positive</keyword>
          <path>ApiniNames/Concepts_Positive</path>
        </li>
        <li Class="Rule_File">
          <keyword>roman</keyword>
          <path>Words/Misc/Numbers_SmallRoman</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>NamerPersonAzuri</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[blue] '[badass]' [badassanimal]</li>
        <li>name->[blue] '[badass]' [negative]</li>
        <li>name->[blue] '[azuribadass]' [badassanimal]</li>
        <li>name->[blue] '[azuribadass]' [negative]</li>
        <li>name(p=0.24)->[blue] '[games]' [badass]</li>
        <li>name(p=0.24)->[badass] '[games]' [blue]</li>
        <li>name->[badassanimal] '[badass]' [blue]</li>
        <li>name->[badassanimal] '[azuribadass]' [blue]</li>
        <li>name->[games] '[negative]' [blue]</li>
        <li>name->[blue] [negative]</li>
        <li>name->[blue] [badass]</li>
        <li>name->[blue] [azuribadass]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>badassanimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>badass</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>
        <li Class="Rule_File">
          <keyword>negative</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>blue</keyword>
          <path>AzuriNames/Colors_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>azuribadass</keyword>
          <path>AzuriNames/Azuri_ExtendedBadass</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
</Defs>