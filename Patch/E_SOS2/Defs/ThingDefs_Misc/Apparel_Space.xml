<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="ArmorMachineableBase">
		<defName>Apini_EVASuit_Light</defName>
		<label>apitech EVA suit</label>
		<description>A space suit made by beeliens for beeliens! Complete with a small set of thrusters for movement in zero G! The suit smells oddly like honey.</description>
		<techLevel>Spacer</techLevel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<researchPrerequisite>ShipSpaceSuits</researchPrerequisite>
			<recipeUsers Inherit="False">
				<li>FabricationBench</li>
			</recipeUsers>
		</recipeMaker>
		<graphicData>
			<texPath>Things/Apparel/Apini_EVASuit_Light</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<statBases>
			<WorkToMake>17000</WorkToMake>
			<MaxHitPoints>340</MaxHitPoints>
			<Mass>10</Mass>
			<ArmorRating_Blunt>0.4</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.4</ArmorRating_Sharp>
			<ArmorRating_Heat>0.75</ArmorRating_Heat>
			<Insulation_Cold>100</Insulation_Cold>
			<Insulation_Heat>25</Insulation_Heat>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>-0.25</MoveSpeed>
			<WorkSpeedGlobal>-0.075</WorkSpeedGlobal>
		</equippedStatOffsets>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>70</costStuffCount>
		<costList>
			<ComponentSpacer>2</ComponentSpacer>
			<Plasteel>10</Plasteel>
			<ApiniHoney>50</ApiniHoney>
		</costList>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
				<li>Hands</li>
				<li>Feet</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/Apini_EVASuit_Light</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<tags>
				<li>Spacer</li>
				<li>EVA</li>
			</tags>
			<defaultOutfitTags>
			</defaultOutfitTags>
		</apparel>
	</ThingDef>
	<ThingDef ParentName="HatMakeableBase">
		<defName>Apini_EVAHelmet_Light</defName>
