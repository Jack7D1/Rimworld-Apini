		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<researchPrerequisite>ShipSpaceSuitsHeavy</researchPrerequisite>
			<skillRequirements>
				<Crafting>11</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="False">
