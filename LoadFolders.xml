		<li IfModActive="consolidatedtraits.kv.rw">1.4/Patched/P_ConsolidatedTraits</li>
		<li IfModActive="Ykara.EPOE">1.4/Patched/P_EPOE</li>
		<li IfModActive="vat.epoeforked">1.4/Patched/P_EPOEForked</li>
		<li IfModActive="sarg.rimbees">1.4/Patched/P_Rimbees</li>
		<li IfModActive="CETeam.CombatExtended">1.4/Patched/P_CombatExtended</li>
		<!--Expanded content for mods, probably with compatability patches as well.-->
		<li IfModActive="Ludeon.Rimworld.Royalty">1.4/Expanded/E_Royalty</li>
		<li IfModActive="kentington.saveourship2">1.4/Expanded/E_SOS2</li>
	</v1.4>
